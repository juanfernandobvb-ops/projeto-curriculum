{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "functions": {
    "api/generate-pdf.js": {
      "memory": 3008,
      "maxDuration": 60,
      "runtime": "nodejs20.x"
    }
  },
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "/api/:path*"
    }
  ]
}
